<div class="container">
  <%= simple_form_for(@estate) do |f| %>
  <div class="form-group">
    <%= f.input :name, placeholder: "Enter the estate name", html: {:class => 'form-control'} %>
    <%= f.input :description, as: :text, html: {:class => 'form-control'} %>
    <%= f.input :photos, as: :attachinary %>
  </div>
  <%= f.submit %>
  <% end %>
</div>
